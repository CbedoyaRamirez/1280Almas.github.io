/**** Card OT ****/
$speed: 0.65s;
$fontSmall: 0.625em;
/* 10px */
$fontMedium: 0.875em;
/* 14px */
$fontLarge: 1.375em;
/* 22px */
$fontLight: 100;

.card_ot {
  position: relative;
  /*grid-area: card;
  align-self: center;
  justify-self: center;*/
  overflow: hidden;
  display: block;
  width: 100%;
  /*max-width: 250px;*/
  height: auto;
  border-radius: 8px;
  cursor: pointer;
  margin-bottom: 10px;
  @include prefix(transition, all $speed ease);

  &:hover {
    box-shadow: 0 8px 17px 2px rgba(0, 0, 0, 0.14),
      0 3px 14px 2px rgba(0, 0, 0, 0.12), 0 5px 5px -3px rgba(0, 0, 0, 0.2);
    text-decoration: none;

    .card__link {
      opacity: 0.6;
    }

    .card__image {
      img {
        @include prefix(transform, scale(1, 1));
        @include prefix(filter, blur(3px));
        opacity: 0.5;
      }
    }

    .card__body {
      background: transparent;
    }

    .card__profile {
      @include prefix(transform, translateY(-15%));

      img {
        @include prefix(transform, scale(1, 1) translateZ(0));
      }
    }

    .card__name {
      background: -webkit-linear-gradient($white, $gray-100);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
    }

    .card__job {
      color: rgba(100, 130, 200, 1);
    }

    .card__bio {
      @include prefix(transform, translateY(15%));
      opacity: 1;
    }
  }

  @include media-breakpoint-up(sm) {
    height: 150px;

    .order-1 {
      order: 2;
    }

    .order-2 {
      order: 1;
    }
  }
}

