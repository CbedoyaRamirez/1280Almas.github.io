    [class*=" tag-"] {
      &::before {
        margin: 0;
      }

      &::after {
        content: "" !important;
      }
    }

    .plus-icon {
      display: none;
      right: 15px;
    }
  }
}

.icon-up-arrow {
  &.rotate-left {
    &::before {
      display: inline-flex;
      transform: rotate(90deg);
    }
  }

  &.rotate-right {
    &::before {
      display: inline-flex;
      transform: rotate(270deg);
    }
  }

  &.rotate-bottom {
    &::before {
      display: inline-flex;
      transform: rotate(180deg);
    }
  }
}

.little-title {
