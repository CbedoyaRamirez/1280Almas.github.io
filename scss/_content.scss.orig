}

.big-top-space {
  margin-top: 2.5rem;
}

.big-padding {
  padding: 2rem 1.5rem;
}

.datos-ev {

  .tag-event-fecha,
  .tag-event-hora,
  .tag-event-int {
    &::before {
      font-size: 2rem;
      font-family: "icoagrosavia";
    }
  }

  .tag-event-fecha {
    &[data-date] {
      &::before {
        content: "\e907"; //icon-calendar
      }

      &::after {
        content: attr(data-date);
        display: block;
      }
    }
  }

  .tag-event-hora {
    &[data-time] {
      &::before {
        content: "\e935"; //iicon-clock
      }

      &::after {
        content: attr(data-time);
        display: block;
      }
    }
  }

  .tag-event-int {
    &[data-tag-event*="Cap"] {
      &::before {
        content: "\e90c"; //icon-capacitacion
      }

      &::after {
        content: attr(data-tag-event);
        display: block;
      }
    }

    &[data-tag-event*="Aca"] {
      &::before {
        content: "\e900"; //icon-academico
      }

      &::after {
        content: attr(data-tag-event);
        display: block;
      }
    }

    &[data-tag-event*="Prom"] {
      &::before {
        content: "\e906"; //icon-agrosavia-mini
      }

      &::after {
        content: attr(data-tag-event);
        display: block;
      }
    }
  }
}

.list-table {
  ul {
    padding: 10px 15px;

    li {
      margin: 0 !important;
      padding: 1em 0;

      .row {
