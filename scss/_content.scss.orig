//
// principal
//
.sinpadding {
  padding: 0 !important;
}

.no-margin:not(.row) {
  margin: 0 !important;
}

.col-sinpadding {

  [class^="col-"],
  [class*=" col-"] {
    padding: 0;
  }
}

.minh-100 {
  height: 100vh;
}

a:not([href]):not([tabindex]),
a:not([href]):not([tabindex]):focus,
a:not([href]):not([tabindex]):hover {
  color: inherit;
  text-decoration: none;
}

.center {
  justify-content: center;
  text-align: center;
}

.centered {
  display: flex;
  /* alineacion vertical */
  align-items: center;
  /* alineacion horizontal */
  justify-content: center;
}

.header-title {
  font-size: 1.5rem;
  font-weight: 100;
  line-height: 1.3;
}

//
// Base de contenido principal
//

.bd-content {
  padding: 0 !important;

  .container-fluid {
    padding: 0;
  }

  .row {
    margin-right: 0;
    margin-left: 0;

    &.normal {
      margin-right: -20px;
      margin-left: -20px;
    }
  }

  .grid {
    .row {
      margin-right: -15px;
      margin-left: -15px;
    }
  }

  .carousel {
    &.home-news {
      .carousel-cell {
        background: transparent;
        width: 80%;
        height: auto;
        margin-right: 0;

        &.size-4 {
          width: 33.3333333333%;
          max-width: 380px;
        }

        &.size-8 {
          width: 66.6666666667%;
          max-width: 760px;
        }

        &.size-full {
          width: 100%;
          max-width: 1140px;
        }

        @include media-breakpoint-down(sm) {

          &.size-4,
          &.size-8 {
            width: 100%;
          }

          [class^="col-"],
          [class*=" col-"] {
            padding: 0;
          }
        }

        .row {
          margin-right: -15px;
          margin-left: -15px;
        }

        .card {
          margin-bottom: 0;
        }
      }
    }
  }

  .grupos-inv {
    padding-top: 3rem;

    &.row {
      margin-right: -15px;
      margin-left: -15px;
    }
  }
}

/* buscador */
.buscador {
  .item {
    padding: 0 $navbar-padding-x;
  }

  .bt-buscador {
    min-width: 115px;
    color: #202121;
    cursor: pointer;
    overflow: hidden;
    height: 52px;
    line-height: 52px;
    @include prefix(transition, all 0.3s ease-in-out);

    i {
      @include prefix(transition, all 0.3s ease-in-out);
    }

    span {
      display: inline-block;
      @include prefix(transition, all 0.3s ease-in-out);

      @include media-breakpoint-down(xs) {
        display: none;
      }
    }

    &:hover {
      i {
        color: $modCyan;
      }

      span {
        color: $gray-600;
      }
    }

    @include media-breakpoint-down(xs) {
      min-width: inherit;
      width: 52px;
    }

    @include media-breakpoint-down(md) {
      display: block;
    }
  }

  @include media-breakpoint-down(sm) {
    border: none;
  }

  @include media-breakpoint-up(md) {
    border-left: 1px solid $border-color;
  }

  @include media-breakpoint-up(lg) {
    border: none;
  }
}

// slider interno img - txt

.row-tiny--4 {
  min-height: 300px;
  height: 33.33333vh;
}

.row-sm-4 {
  min-height: 150px;
  height: 19.66vh;
}

@include media-breakpoint-up(md) {
  .row-small--7 {
    min-height: 450px;
    height: 58.33333vh !important;
  }

  .row-md-7 {
    min-height: 300px;
    height: 39.32vh !important;
  }

  .row-sm-7 {
    min-height: 150px;
    height: 19.66vh !important;
  }

  .row-small-7 {
    height: calc(100% - 55px) !important;
  }
}

.bg-cover {
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
  height: 100%;
  width: 100%;
}

.texture-overlay {
  background-color: transparent;
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background-image: url(../images/bg/grid.png);
  @include prefix(transition, all 0.3s ease-in-out);
}

.carousel-container {
  position: relative;

  .parent-el {
    width: 100%;
    max-width: 100%;
    margin: auto;

    .two-col {
      display: flex;
      align-items: center;

      .has-img {
        opacity: 0;
        filter: alpha(opacity=0);
        /* For IE8 and earlier */
        @include prefix(transition, all 0.3s ease-in-out);
        transform: scale(1);
        position: relative;
        z-index: 0;
        /*@include prefix(transition-delay, 300ms);*/

        .hover-item {
          visibility: hidden;
          opacity: 0;
          position: absolute;
          @include prefix(transition, opacity 0.3s ease-in-out);

          a {
            &:hover {
              opacity: 0.7;
            }
          }

          &.bottom {
            bottom: 25px;
          }

          &.top {
            top: 25px;
          }

          &.left {
            left: 25px;
          }

          &.right {
            right: 25px;
          }

          &.center {
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
          }

          .bt-link {
            width: 35px;
            height: 35px;
            border: 1px solid rgba(255, 255, 255, 0.5);
            border-radius: 50%;
          }

          .bt-link {
            border-radius: 50%;
            height: 40px;
            width: 40px;
            color: $white;
            display: flex;
            align-items: center;
            justify-content: center;
            @include prefix(transition, opacity 0.3s ease-in-out);

            .icon {
              font-size: 18px;
              margin: 0;
            }
          }
        }

        .icon {
          font-size: 40px;
          color: $white;
          margin: 0;
        }

        &:hover {
          transform: scale(1.05);

          a {
            text-decoration: none;
          }

          .texture-overlay {
            background-color: rgba($a-savia, 0.45);
            transform: scale(1);
          }

          .hover-item {
            visibility: visible;
            opacity: 1;
          }
        }

        @include media-breakpoint-down(sm) {
          opacity: 1;
          filter: alpha(opacity=100);
          /* For IE8 and earlier */
          order: 1;
        }
      }

      .has-content {
        overflow: hidden;
        position: relative;
        display: flex;
        flex-wrap: wrap;
        align-items: center;
        justify-content: space-between;
        padding: 0;
        z-index: 1;

        .box-info {
          display: flex;
          flex-flow: row wrap;
          overflow: hidden;
          width: 100%;

          .box-tagDate {
            margin-bottom: 1em;
            opacity: 0;
            @include prefix(transform, translateY(10px));
            @include prefix(transition, all 100ms ease);

            .date-box {
              color: $gray-date;
              font-size: 0.75rem;

              .ev {
                &[data-time] {
                  &::after {
                    content: attr(data-time);
                    color: $green-dark;
                  }
                }
              }
            }

            [data-time] {
              &::after {
                content: attr(data-time);
                color: $ot;
              }
            }

            [data-categoria] {
              &::after {
                content: attr(data-categoria);
                text-transform: uppercase;
              }
            }
          }

          h1 {
            font-size: 1.8rem;
            letter-spacing: 2px;
            color: $white;
            margin: 0;
            @include prefix(transform, translateY(100%));
            position: relative;
            @include prefix(transition, all 500ms ease);
            display: inline-block;
          }

          hr {
            margin: 0.5em 0em;
            height: 1px;
            background: $hr-border-slider;
            border: none;
            width: 0;
            max-width: 100%;
            @include prefix(transition, all 2500ms ease);
          }

          p {
            color: $white;
            margin-top: 1rem;
            line-height: 1.8;
            transform: translateY(15px);
            position: relative;
            opacity: 0;
            @include prefix(transition, all 400ms ease);
          }

          .contenido {
            position: absolute;
            top: 0;
            display: flex;
            align-items: center;
            justify-content: center;
            /*height: calc(58.33333vh - 7vh);*/
            width: 100%;

            .location {
              p {
                margin-top: 0;
              }
            }

            @include media-breakpoint-down(sm) {
              position: relative;
              height: 360px;
              align-items: flex-start;
            }
          }

          .content {
            padding: 30px 6vw;

            @include media-breakpoint-down(sm) {
              padding: 30px 2.5vw;
            }
          }

          .footer {
            position: absolute;
            bottom: 0;
            display: flex;
            justify-content: space-between;
            padding: 11px 20px 10px 20px;
            font-size: 12px;
            width: 100%;

            &:before {
              content: "";
              position: absolute;
              top: 0;
              left: 0;
              width: 100%;
              height: 1px;
              padding: 0 20px;
              background: $hr-border-color;
            }

            &:after {
              content: "";
              display: block;
              clear: both;
            }

            .box-left {
              .box-byuser {
                a {
                  color: $gray-date;

                  &:hover {
                    color: $ot;
                  }
                }

                .by {
                  color: $gray-date;
                }
              }
            }
          }
        }

        @include media-breakpoint-down(sm) {
          order: 2;
        }
      }

      @include media-breakpoint-down(sm) {
        display: flex;
        flex-direction: column;
      }
    }
  }

  .is-selected {
    .parent-el {
      .two-col {
        .has-img {
          opacity: 1;
          filter: alpha(opacity=100);
          /* For IE8 and earlier */
          /*@include prefix(transition-delay, 300ms);*/
        }

        /*.the-img {
        overflow: hidden;
        img {
          position: relative;
          transform: translateX(0%);
          transition-delay: 500ms;
        }
      }*/
        .has-content {
          .box-info {
            h1 {
              @include prefix(transform, translateY(0%));
              @include prefix(transition-delay, 400ms);
            }

            .box-tagDate {
              transform: translateY(0px);
              opacity: 1;
              @include prefix(transition-delay, 400ms);
            }

            hr {
              width: 100%;
              max-width: 100%;
              @include prefix(transition-delay, 500ms);
            }

            p {
              @include prefix(transform, translateY(0%));
              @include prefix(transition-delay, 500ms);
              position: relative;
              opacity: 1;

              &:nth-of-type(2) {
                color: red;
                @include prefix(transition-delay, 700ms);
              }

              &:nth-of-type(3) {
                @include prefix(transition-delay, 900ms);
              }
            }
          }
        }
      }
    }
  }

  .carousel-status {
    position: absolute;
    top: 0;
    color: $white;
    padding: 15px 25px 25px 15px;
    background: rgba(0, 0, 0, 0.2);
    border-radius: 0 0 100%;

    &.status-right {
      right: 0;
      padding: 15px 15px 25px 25px;
      border-radius: 0 0 0 100%;
    }
  }

  .flickity-button {
    background: rgba(0, 0, 0, 0.1);
  }

  .flickity-button-icon {
    fill: $white;
  }

  @include media-breakpoint-down(sm) {
    .flickity-prev-next-button {
      top: 25%;
    }
  }
}

.moreInfo {
  display: flex;
  justify-content: space-between;
  font-size: 12px;
  width: 100%;
}

.moreInfoBottom {
  @include media-breakpoint-up(lg) {
    &.hinfoBottom {
      bottom: 50px;
    }
  }

  @include media-breakpoint-down(sm) {
    &.hinfoBottom {
      .box-left {
        display: none;
      }

      .box-right {
        justify-content: flex-end;
        width: 100%;
      }
    }
  }
}

.box-left {
  display: flex;
  flex: 1;
  align-items: center;

  .box-byuser {
    display: flex;
    align-items: center;

    .item {
      position: relative;
    }

    a {
      color: $body-color;
      @include prefix(transition, all 0.3s ease-in-out);

      &:hover {
        color: $ot;
        text-decoration: none;
      }
    }

    img {
      border-radius: 50%;
    }

    .by {
      flex: 1;
      font-size: 0.625rem;
      padding: 0 5px 0 8px;
      text-overflow: ellipsis;
      white-space: nowrap;
      overflow: hidden;

      strong {
        text-transform: uppercase;
      }
    }
  }
}

.box-right {
  display: flex;
  align-items: center;
}

.share-list {
  list-style-type: none;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
}

.list-tags {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-content: center;
  align-items: center;
  margin: 0;
  padding: 0;

  &.text-light {
    li {
      a {
        color: $white;
        background-color: rgba($body-color, 0.5);
        display: flex;
        align-items: center;
        justify-content: center;

        &.btn {
          border-radius: 50%;
          height: 42px;
        }

        &:hover {
          background-color: rgba($v-agro, 0.5);
          height: 42px;
          width: 42px;
        }
      }

      i {
        margin-right: 0;
      }
    }
  }

  li {
    list-style: none;
    margin: 0 3px 0 0;
    margin-left: 5px;

    .box-tooltip {
      display: flex;
      align-items: center;
      height: 15px;
      padding: 1px 5px 2px;
      line-height: normal;
      font-size: 0.625rem;
      font-weight: 300;
      letter-spacing: 1px;
      color: #fff;
      border-radius: 35px;
      cursor: default;
      text-transform: uppercase;

      &.btn-na {
        color: transparent;
      }
    }

    .dias-ev {
      display: flex;

      &[data-date] {
        &::after {
          content: attr(data-date);
          color: $v-agro;
          display: flex;
          align-items: center;
          justify-content: center;
          padding: 0px 8px;
          line-height: 20px;
          font-size: 0.75rem;
          background-color: $light;
          border-radius: 5px;
          cursor: default;
        }
      }
    }
  }
}

/* ------------truncate---------------- */
.truncate-slide-title {
  height: 75px;
}

.truncate-slide-text {
  height: 68px;

  @include media-breakpoint-up(lg) {
    height: 87px;
  }

  @include media-breakpoint-up(xl) {
    height: calc(68px * 2);
  }
}

.truncate-slide-text-ev {
  height: 30px;

  @media screen and (min-height: 200px) {
    height: 60px !important;
  }

  @media screen and (min-height: 500px) {
    height: 90px !important;
  }

  @media screen and (min-height: 1000px) {
    height: 130px !important;
  }

  @include media-breakpoint-down(sm) {
    height: 30px !important;
  }

  @include media-breakpoint-up(lg) {
    height: 87px;
  }

  @include media-breakpoint-up(xl) {
    height: calc(68px * 2);
  }
}

.truncate-banner-title {
  height: 64px;

  @include media-breakpoint-up(md) {
    height: 308px;
  }

  @include media-breakpoint-up(lg) {
    height: 140px;
  }

  @include media-breakpoint-up(xl) {
    height: 64px;
  }
}

.truncate-banner-text {
  height: 192px;

  @include media-breakpoint-only(sm) {
    height: 96px;
  }

  @include media-breakpoint-only(md) {
    height: 408px;
  }

  @include media-breakpoint-only(lg) {
    height: 240px;
  }

  @include media-breakpoint-up(xl) {
    height: 126px;
  }
}

.card-title {
  height: 60px;
}

.card-text {
  height: 50px;
}

/*-------------------------------------------*/
//share

.modal-dialog {
  min-height: calc(100vh - 60px);
  display: flex;
  flex-direction: column;
  justify-content: center;
  overflow: auto;

  @media (max-width: 768px) {
    min-height: calc(100vh - 20px);
    margin: 0;
    max-width: 100%;
  }
}

#modal-share,
#modal-info {
  .modal-content {
    .modal-header {
      border-bottom: 1px solid transparent;
      display: block;
      text-align: center;
      width: 100%;

      .close {
        position: absolute;
        top: 18px;
        right: 18px;
      }

      p {
        margin-bottom: 0;
        text-align: left;
      }
    }

    @include media-breakpoint-down(md) {
      border: none;
      border-radius: 0;
      outline: 0;
      justify-content: center;
      overflow-y: hidden;
      align-items: center;
      padding: 70px 20px 50px 20px;
      min-height: 100vh;

      .modal-body {
        flex: initial;
        width: 100%;

        [class^="col-"],
        [class*=" col-"] {
          padding: 0;
        }
      }
    }
  }

  .modal-title {
    padding-right: 20px;
    padding-left: 20px;
    font-weight: 600;
  }

  li {
    margin-bottom: 15px;
    color: $white;

    a {
      text-transform: uppercase;
      color: $white;
      font-weight: 700;
      width: 100%;
      max-width: 300px;
    }

    a.btn-default {
      @include button-variant($gray-200, $gray-200);
      color: #444;
    }
  }
}

/* -----------------------------------TOOLTIP colores---------------------------------------------- */

.tooltip {
  &.ot {
    .arrow {
      &:before {
        border-top-color: $ot;
      }
    }

    .tooltip-inner {
      background-color: $ot;
    }
  }

  &.ev {
    .arrow {
      &:before {
        border-top-color: $ev;
      }
    }

    .tooltip-inner {
      background-color: $ev;
    }
  }

  &.ci {
    .arrow {
      &:before {
        border-top-color: $ci;
      }
    }

    .tooltip-inner {
      background-color: $ci;
    }
  }

  &.pos {
    .arrow {
      &:before {
        border-top-color: $pos;
      }
    }

    .tooltip-inner {
      background-color: $pos;
    }
  }

  &.ri {
    .arrow {
      &:before {
        border-top-color: $indigo;
      }
    }

    .tooltip-inner {
      background-color: $indigo;
    }
  }

  &.io {
    .arrow {
      &:before {
        border-top-color: $io;
      }
    }

    .tooltip-inner {
      background-color: $io;
    }
  }

  &.in {
    .arrow {
      &:before {
        border-top-color: $in;
      }
    }

    .tooltip-inner {
      background-color: $in;
    }
  }

  &.na {
    .arrow {
      &:before {
        border-top-color: $a-savia;
      }
    }

    .tooltip-inner {
      background-color: $a-savia;
    }
  }

  &.su {
    .arrow {
      &:before {
        border-top-color: $orange;
      }
    }

    .tooltip-inner {
      background-color: $orange;
    }
  }
}

/* ----------------------------------- Banner ---------------------------------------------- */

// Some helper mixins
// ----------------------------------------------------

@mixin small-screen() {
  @media screen and (min-width: 768px) {
    @content;
  }
}

// Basic styling
// ----------------------------------------------------

.hero {
  position: relative;
  width: 100%;
  height: 100%;

  .flickity-page-dots {
    bottom: 15px;
  }
}

.hero-cell {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  /*&:after {
    @include small-screen {
      display: block;
      content: "";
      position: absolute;
      height: 100%;
      width: 1px;
      top: 0;
      bottom: 0;
      left: 50%;
      margin: 0;
      padding: 0;
      border-left: 1px solid rgba(255, 255, 255, .3);
    }
  }*/
}

.hero-cell-bg {
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;

  img {
    width: 100%;
  }
}

.hero-cell-content {
  position: absolute;
  bottom: 60px;
  left: 30px;
  right: 30px;
  z-index: 100;

  @include small-screen {
    position: static;
    bottom: auto;
    left: auto;
    right: auto;
  }
}

.hero-cell-text-left,
.hero-cell-text-right {
  @include small-screen {
    position: absolute;
    top: 0;
    overflow: hidden;
    z-index: 100;
    display: flex;
    align-items: center;
    height: 600px;
  }
}

.hero-cell-text-left {
  @include small-screen {
    left: 20%;
    right: 40%;
    text-align: right;
  }
}

.hero-cell-text-right {
  @include small-screen {
    left: 60%;
    right: 10%;
  }
}

.hero-cell-text {
  color: #ffffff;

  @include small-screen {
    padding: 30px;
  }
}

.hero-cell-title {
  margin: 0 0 15px 0;
  padding: 0;
  line-height: 1;
}

.hero-subhead {
  font-size: 0.625rem;
  margin: 0;
  text-transform: uppercase;
  line-height: 1;
}

.hero-cell-angle {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;

  @include small-screen {
    left: 45%;
  }
}

.hero-cell-angle-svg {
  position: absolute;
  right: 0;
  bottom: -30%;
  opacity: 0;
  width: 200%;
  transform: translate(20%, 10%);
  transition: transform 300ms ease-out, opacity 300ms ease-out;
  transition-delay: 300ms;

  @include small-screen {
    left: -35%;
    bottom: 0;
    width: 200%;
  }

  @include small-screen {
    left: -35%;
    bottom: 0;
    width: 200%;
  }

  @include media-breakpoint-only(xs) {
    bottom: -55%;
    left: -180%;
    width: 460%;
  }

  @include media-breakpoint-only(sm) {
    left: -35%;
    bottom: 0;
    width: 200%;
  }

  @include media-breakpoint-up(md) {
    left: -120%;
    bottom: 0;
    width: 340%;
  }

  @include media-breakpoint-only(xl) {
    left: -50%;
    width: 200%;
  }
}

.hero-cell-angle-path {
  fill: transparent;
  opacity: 0.9;
}

.light-blue .hero-cell-angle-path {
  fill: #3db7e4;
}

