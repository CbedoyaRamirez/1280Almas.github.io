    font-size: 1em;
    outline: none;
    box-sizing: border-box;
    text-transform: uppercase;
    color: #fff !important;
    text-decoration: none !important;
    font-weight: 200;

    &:hover {
      background-color: #1f9c83;
    }

    &::before {
      content: "Preincripción";
    }

    &.disabled {
      background-color: red;

      &::before {
        content: "Evento Privado";
      }
