/* ------------------------------------------------------------------- 
 * Nosotros - MARCO ESTRATEGICO CORPORATIVO (MEC)
 * ------------------------------------------------------------------- */

.mec {
  .objetivosE {
    margin-top: 3rem;

    li {
      counter-increment: obj-list;
      position: relative;

      .info {
        .title-obj {
          font-weight: bold;
          font-size: 1.2rem;

          p {
            color: $white;
            display: inline-block;
            padding: 5px 0 5px 5px;

            &::after {
              content: counter(obj-list);
              background: rgba($color: $white, $alpha: 0.2);
              padding: 8px 15px;
              margin-left: 20px;
            }
          }
        }

        hr {
          border-top-width: 2px;
          position: relative;

          &::after {
            content: "";
            width: 10px;
            height: 10px;
            background: #c3c4ca;
            display: block;
            position: absolute;
            top: -6px;
            border-radius: 50%;
          }
        }
      }

      .obj-img {
        position: relative;
      }

      &:nth-child(-n + 2) {
        .obj-img {
          align-self: flex-end;
        }
      }

      &:nth-child(odd) {
        .info {
          padding-right: 15px;

          .title-obj {
            text-align: left;
          }

          hr {
            &::after {
              right: 0;
            }
          }
        }
      }

      &:nth-child(even) {
        .info {
          padding-left: 15px;

          hr {
            &::after {
              content: "";
              left: 0;
            }
          }

          .title-obj {
            text-align: right;
          }

          p {
            text-align: right;
          }
        }
      }

      @include media-breakpoint-down(sm) {
        .info {
          padding: 0 !important;
          order: 2;
        }

        .obj-img {
          order: 1;
          text-align: center;
          top: 0;
        }
      }
    }
  }

  .us-mision,
  .us-vision {
    position: relative;
    display: flex;

    .info {
      padding: 0 6rem 0 0;
      z-index: 1;
    }

    img {
      padding: 0 15px;
      position: absolute;
      bottom: 0;
      right: 0;
      z-index: 0;
    }
  }

  .us-vision {
    .info {
      padding: 0 0 0 6rem;
    }

    p {
      &:last-child {
        margin-bottom: 4rem;
      }
    }

    @include media-breakpoint-down(md) {
      .info {
        padding: 0;
      }
    }
  }

  .us-valores {
    margin: 2rem 0 5rem;

    li {
      display: flex;
      flex-direction: row;
      margin-bottom: 0;
      position: relative;

      h5 {
        color: $white;
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 50px 0px;
        padding: 10px 20px;
        margin-bottom: 0;
        height: 100%;
        font-weight: bold;

        &::before {
          content: "";
          position: absolute;
          bottom: 0;
          left: 0;
          width: 50%;
          height: 100%;
          background: rgba(255, 255, 255, 0.2);
          z-index: 1;
        }
      }

      p {
        margin-bottom: 0;
        padding: 1rem 0;
      }

      @for $i from 1 through 100 {
        $j: 100 - $i + 1; // reverse nth-child order math

        &:nth-child(#{$i}) {
          z-index: $j;
        }
      }
    }

    @include media-breakpoint-up(md) {
      li {
        p {
          padding: 1rem 0 1rem 1rem;
        }
      }
    }
  }
}

.shadow-left {
  background: $white;
  position: relative;

  &::before {
    z-index: -1;
    position: absolute;
    content: "";
    bottom: 15px;
    left: 13%;
    width: 50%;
    top: 80%;
    max-width: 300px;
    background: #777;
    -webkit-box-shadow: 0 15px 10px #777;
    -moz-box-shadow: 0 15px 10px #777;
    box-shadow: 0 15px 10px #777;
    @include prefix(transform, rotate(-2deg));
  }

  @include media-breakpoint-down(sm) {
    background: transparent;

    &::before {
      content: none;
    }
  }
}

// *********************** //
// timeline
// *********************** //

.mec-timeline {
  position: relative;
  margin: 2rem 0 5rem;

  &::before {
    /* this is the vertical line */
    content: "";
    position: absolute;
    top: 0;
    left: 19px;
    height: 100%;
    width: 2px;
    background: rgba(#000, 0.1);
  }

  li {
    display: block;
    position: relative;
    margin: 0.6rem 0;
    width: 100%;

    &::before {
      content: "";
      position: absolute;
      left: 20px;
      margin-left: -10px;
      top: 12px;
      width: 20px;
      height: 20px;
      border: 3px solid $white;
      border-radius: 50%;
      background-color: #c3c4ca;
    }

    &:first-child {
      margin-top: 0;
    }

    &:last-child {
      margin-bottom: 2rem;
    }

    &:nth-child(4n-3) {
      &::before {
        background-color: $ins;
      }
    }

    &:nth-child(4n-2) {
      &::before {
        background-color: $pasto;
      }
    }

    &:nth-child(4n-1) {
      &::before {
        background-color: $cielo;
      }
    }

    &:nth-child(4n) {
      &::before {
        background-color: $tomate;
      }
    }

    .cd-timeline-content {
      //color: $white;
      position: relative;
      margin-left: 60px;
      background: $gray-200;
      padding: 1rem;
      box-shadow: 0 3px 0 darken($gray-200, 5%);

      p {
        margin: 0;
      }

      &::before {
        content: "";
        position: absolute;
        top: 16px;
        right: 100%;
        height: 0;
        width: 0;
        border: 7px solid transparent;
        border-right: 7px solid $gray-200;
      }
    }
  }

  @include media-breakpoint-up(md) {
    &::before {
      left: 50%;
      margin-left: -1px;
    }

    li {
      &::before {
        left: 50%;
        top: 22px;
      }

      /*&:nth-child(4n-3) {
        .cd-timeline-content {
          &::before {
            border-color: transparent;
            border-left-color: $ins;
          }
        }
      }

      &:nth-child(4n-1) {
        .cd-timeline-content {

          &::before {
            border-color: transparent;
            border-left-color: $cielo;
          }
        }
      }*/

      .cd-timeline-content {
        margin-left: 0;
        width: 45%;

        &::before {
          top: 24px;
          left: 100%;
          border-color: transparent;
          border-left-color: $gray-200;
        }
      }

      &:nth-child(even) {
        .cd-timeline-content {
          float: right;

          &::before {
            top: 24px;
            left: auto;
            right: 100%;
            border-color: transparent;
            border-right-color: $gray-200;
          }
        }
      }
    }
  }
}

.box-mec {
  background: transparent;
  border-radius: 10px 10px 10px 0;
  padding: 20px;
  margin-bottom: 2rem;
  color: #fff;
  position: relative;
  text-align: center;

  &::before {
    content: "";
    position: absolute;
    bottom: -20px;
    left: 0;
    height: 0;
    width: 0;
    border: 10px solid;
    //border-color: darken(#45a4a4, 10%);
    border-bottom-color: transparent;
    border-left-color: transparent;
  }

  h4 {
    font-size: 1rem;
    font-weight: bold;
    margin-top: 0;
    margin-bottom: 1.5rem;
    text-align: center;
  }

  p {
    margin: 0;
  }

  .mec-lineamientos {
    display: flex;
    flex-flow: row wrap;
    justify-content: center;
    margin-bottom: 0;
    border: 1px dashed #fff;
    border-radius: 10px;
    padding: 20px;

    li {
      display: flex;

      a {
        color: $white;
        text-decoration: none;
        border: 1px solid #fff;
        border-radius: 10px;
        padding: 10px;
        margin: 0 10px;
        @include prefix(transition, all 0.3s ease-in-out);

        &:hover {
          background: $body-bg;
          color: $body-color;
        }
      }

      &:last-child {
        display: block;
        text-align: center;
        width: 100%;
        margin-bottom: 0;
      }
    }
  }

  span {
    display: block;
    text-align: center;
    font-weight: bold;
    font-size: 1.25rem;
  }
}

.apuestas {
  margin: 0 0 5rem;

  li {
    .title {
      text-align: center;
      color: $white;
      padding: 10px 0;
      margin-bottom: 15px;

      &::before {
        content: "";
        background: $ins;
        position: absolute;
        top: 0;
        left: calc(50% - 35%);
        height: 100%;
        width: 70%;
        border-radius: 0 0 10px 10px;
        border: 10px solid;
        border-color: rgba($white, 0.2);
        //border-bottom-color: transparent;
        //border-left-color: transparent;
        z-index: -1;
      }

      h4 {
        font-size: 1.25rem;
        margin: 0;
      }
    }

    @for $i from 10 through 30 {
      $j: 3 - $i / 10 + 1; // math 1+1+1...
      $x: $i + 60%;
      $y: $x/2;

      &:nth-child(#{$j}) {
        .title {
          &::before {
            width: $x;
            left: calc(50% - #{$y});
          }
        }
      }
    }

    &:first-child {
      .title {
        &::before {
          border-radius: 10px;
        }
      }
    }

    &:nth-child(3n-2) {
      .title {
        &::before {
          background: #70b025;
        }
      }
    }

    &:nth-child(3n) {
      .title {
        &::before {
          background: $cielo;
        }
      }
    }

    @include media-breakpoint-up(md) {
      .title {
        margin-bottom: 0;

        &::before {
          height: calc(100% + 12px);
        }

        &::after {
          content: "";
          background: rgba(0, 0, 0, 0.2);
          position: absolute;
          top: calc(50% - 1px);
          right: 0;
          height: 2px;
          width: 20%;
        }
      }

      p {
        margin: 0;
      }
    }
  }
}

.estrategia {
  margin: 0 0 5rem;
}

.metasImpacto {
  margin: 0 0 2rem;

  [class^="col-"],
  [class*=" col-"] {
    //padding: 0 .5px;
  }

  h4 {
    margin: 1rem 0;
    line-height: 2.188rem;
    font-size: 1.2rem;
    font-weight: bold;
  }

  table {
    text-align: center;
  }

  ul {
    margin-top: 1.5rem;

    li {
      margin: 0;

      p {
        margin: 0;
      }
    }
  }
}

/* ------------------------------------------------------------------- 
 * MENU INTERNO - Nosotros / Redes
 * ------------------------------------------------------------------- */

.box-menu {
  margin-top: 2rem;

  a {
    color: inherit;
    text-decoration: none;
  }

  .box {
    padding: 2rem 0.5rem;
    box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.2);
    position: relative;
    overflow: hidden;
    @include prefix(transition, all 0.3s ease-in-out);

    &.block {
      margin-bottom: 30px;
      background: #fcfcfc;
      border-radius: 5px;
    }

    .profile-picture {
      border-radius: 100%;
      overflow: hidden;
      -webkit-box-sizing: content-box;
      -moz-box-sizing: content-box;
      box-sizing: content-box;
      box-shadow: 1px 2px 5px rgba(0, 0, 0, 0.2);

      img {
        width: 100%;
      }

      &.big-profile-picture {
        margin: 0 auto;
        border: 5px solid #50597b;
        width: 150px;
        height: 150px;
      }

      .icon-cont {
        position: absolute;
        margin: 0;
        left: 50%;
        -webkit-transform: translateX(-50%);
        transform: translateX(-50%);
        width: 150px;
        height: 150px;
        border-radius: 50%;
        box-shadow: 0 0 0 0px rgba(255, 255, 255, 0.5),
          0 0 0 0px rgba(3, 108, 129, 0.5);

        i {
          color: $a-savia;
          font-size: 3rem;
          margin: 0;
        }
      }

      @include media-breakpoint-down(md) {
        &.big-profile-picture {
          width: 100px;
          height: 100px;
        }

        .icon-cont {
          width: 100px;
          height: 100px;
        }
      }

      @include media-breakpoint-down(sm) {
        &.big-profile-picture {
          width: 50px;
          height: 50px;
        }

        .icon-cont {
          width: 50px;
          height: 50px;

          i {
            font-size: 2rem;
          }
        }
      }
    }

    h1.user-name {
      margin: 25px 0 16px !important;
      font-size: 1.375rem !important;
      line-height: 1.4 !important;
      text-align: center;
    }

    .profile-description {
      text-align: center;
    }

    &:hover {
      box-shadow: 0 8px 50px rgba(0, 0, 0, 0.2);
      transform: scale(1.05);
      @include prefix(transition, box-shadow 0.2s ease);
      @include prefix(transform, 0.2s ease);
      @include prefix(transition, all 0.3s ease-in-out);

      .profile-picture {
        @include prefix(animation, shady 4s linear infinite);
      }
    }

    @include media-breakpoint-down(sm) {
      h1.user-name {
        font-size: 1rem !important;
        margin: 0 !important;
        text-align: left;
      }

      .profile-description {
        display: none;
      }
    }
  }

  @include media-breakpoint-down(sm) {

    [class^="col-"],
    [class*=" col-"] {
      padding: 0;
    }
  }

  &.redes-menu {
    .box {
      &:hover {
        .profile-picture {
          &.frutales {
            @include prefix(animation, shady_frutales 4s linear infinite);
          }

          &.hortalizas {
            @include prefix(animation, shady_hortalizas 4s linear infinite);
          }

          &.permanentes {
            @include prefix(animation, shady_permanentes 4s linear infinite);
          }

          &.cacao {
            @include prefix(animation, shady_cacao 4s linear infinite);
          }

          &.ganaderia {
            @include prefix(animation, shady_ganaderia 4s linear infinite);
          }

          &.transitorios {
            @include prefix(animation, shady_transitorios 4s linear infinite);
          }

          &.raices {
            @include prefix(animation, shady_raices 4s linear infinite);
          }
        }
      }
    }

    .user-name {
      span {
        font-size: 1rem;
        display: block;
        font-weight: 400;
      }
    }
  }
}

/* ------------------------------- 
 * Nosotros - Centros de Investigación
 * ------------------------------- */

@mixin aspect-ratio($width, $height) {
  position: relative;

  &:before {
    display: block;
    content: "";
    width: 100%;
    padding-top: ($height / $width) * 100%;
  }

  >.content {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
  }
}

.side-container {
  max-height: 520px;
  overflow-y: scroll;
  overflow-x: hidden;
}

.ratio16-9 {
  @include aspect-ratio(16, 9);

  #map {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: #ccc;
  }
}

#listContainer {
  padding: 0;
  width: 100%;
}

.input-group-btn:last-child>.btn,
.input-group-btn:last-child>.btn-group {
  z-index: 2;
  margin-left: -1px;
}

.btn-default {
  color: #333;
  background-color: #fff;
  border-color: #ccc;
}

.input-group-btn>.btn+.btn {
  margin-left: -1px;
}

.input-group .form-control:last-child,
.input-group-addon:last-child,
.input-group-btn:first-child>.btn-group:not(:first-child)>.btn,
.input-group-btn:first-child>.btn:not(:first-child),
.input-group-btn:last-child>.btn,
.input-group-btn:last-child>.btn-group>.btn,
.input-group-btn:last-child>.dropdown-toggle {
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
}

.input-group .form-control:first-child,
.input-group-addon:first-child,
.input-group-btn:first-child>.btn,
.input-group-btn:first-child>.btn-group>.btn,
.input-group-btn:first-child>.dropdown-toggle,
.input-group-btn:last-child>.btn-group:not(:last-child)>.btn,
.input-group-btn:last-child>.btn:not(:last-child):not(.dropdown-toggle) {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.has-feedback {
  position: relative;
}

.form-control-feedback {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 2;
  display: block;
  width: 34px;
  height: 34px;
  line-height: 34px;
  text-align: center;
  pointer-events: none;
}

.form-control-clear {
  z-index: 10;
  pointer-events: auto;
  cursor: pointer;
}

.hidden {
  display: none !important;
}

.box-directory {
  li {
    margin-bottom: 0 !important;

    &:nth-child(even) {
      .card {
        background-color: #fbfbfb;
      }
    }

    .card {
      border: $card-border-width solid $card-border-color;
      padding: 0.938rem;
    }
  }

  @include media-breakpoint-down(sm) {
    li {
      padding-right: 0;
      padding-left: 0;
    }
  }
}

/* ------------------------------------------------------------------- 
 * Junta Directiva
 * ------------------------------------------------------------------- */

#directiva {
  section:first-child .position {
    margin-top: 0;
  }

  section .team:last-child {
    margin-bottom: 32px;
  }

  .position {
    background: $a-savia;
    border-radius: 5px;
    color: $white;
    margin: 2rem 0;
    padding: 1rem 0.5rem;
    text-align: center;

    h1 {
      margin: 0 !important;
      font-size: 1.375rem !important;
    }
  }

  .team {
    border: 1px solid rgba(133, 133, 133, 0.15);
    margin-top: 80px;
    margin-bottom: 122px;
    @include prefix(box-shadow, 0 0 8px rgba(0, 0, 0, 0.09));

    .teamimg {
      margin-top: -80px;
      padding: 0;
      height: 250px;
      box-shadow: 0px 30px 30px -20px rgba(169, 169, 169, 0.9),
        inset 0 0 0 1000px rgba(67, 52, 109, 0.2);

      img {
        display: block;
        width: 100%;
        height: 100%;
        object-fit: cover;
        object-position: 0 0;
      }
    }

    .teaminfo {
      padding: 0 15px;

      h2,
      .cargo {
        text-align: center;
      }

      h2 {
        margin-top: 1.8rem;
      }

      .cargo {
        color: $a-savia;
      }
    }
  }

  .teamsocial {
    background-color: $limeGreen;
    border-radius: 10px;
    position: relative;
    bottom: -14px;
    left: 0;

    ul {
      padding: 0;
      margin: 0;

      li {
        list-style: none outside none;
        display: inline-block;
        margin-bottom: 0;

        a {
          &:hover {
            opacity: 1;
          }
        }

        i {
          font-size: 22px;
          margin: 0;
          text-align: center;
        }
      }
    }
  }

  .facebook-team,
  .twitter-team,
  .instagram-team {
    color: $gray-500;
  }

  .facebook-team:hover {
    color: $fb;
  }

  .twitter-team:hover {
    color: $tw;
  }

  .instagram-team:hover {
    color: $ins;
  }

  @include media-breakpoint-up(md) {
    .position {
      margin-bottom: 0;
    }

    .team {
      border: none;
      margin-top: 1.5rem;
      margin-bottom: 3rem;
      @include prefix(box-shadow, none);

      .teamimg {
        margin-top: 0;
        height: auto;
        z-index: 1;
      }

      .teaminfo {
        padding: 0 30px;
      }
    }
  }
}

/* ------------------------------------------------------------------- 
 * Grupos de investigacion
 * ------------------------------------------------------------------- */

.group-grid {
  padding-bottom: 5rem;

  .group-column {
    z-index: 0;
    position: relative;
    background: $white;
    box-shadow: 0 0 0 1px #e2e9ed;
    padding: 0;
    transition: box-shadow 0.2s ease, transform 0.2s ease, z-index 0s 0.2s ease;

    &:hover {
      z-index: 1;
      box-shadow: 0 8px 50px rgba(#000, 0.2);
      transform: scale(1.05);
      transition: box-shadow 0.2s ease, transform 0.2s ease, z-index 0s 0s ease;

      .social {
        opacity: 1;
        bottom: -20px;
      }
    }

    a {
      text-decoration: none;
      width: 100%;
    }

    .user {
      width: 100%;
      text-align: center;
      padding: 2.5rem 1rem;

      &-avatar {
        width: 98px;
        height: 98px;
        border-radius: 100%;
        margin: 0 auto 20px;
        overflow: hidden;

        img {
          display: block;
          width: 100%;
          max-width: 100%;
        }
      }

      &-name {
        color: #313435;
        font-size: 1.375rem;
        font-weight: bold;
        line-height: 1.6rem;
      }

      &-title {
        color: #6f808a;
        font-size: 0.875rem;
        font-weight: bold;
        margin-top: 10px;
      }

      &-work {
        color: #6f808a;
        font-size: 0.75rem;
      }
    }

    .social {
      margin: 0;
      padding: 0;
      opacity: 0;
      position: absolute;
      bottom: 0;
      @include prefix(transition, all 0.3s ease);

      &-item {
        display: inline-block;
        margin: 0 10px;

        a {
          background: $white;
          box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18),
            0 4px 15px 0 rgba(0, 0, 0, 0.15);
          width: 47px;
        }
      }
    }
  }
}

/* ------------------------------------------------------------------- 
 * Formulario atención al cliente
 * ------------------------------------------------------------------- */
#atencionUsuario {
  margin-bottom: 5rem;

  textarea {
    min-height: 250px;
  }
}

#solicitudBac,
#preinscripcion {
  padding-top: 1rem;

  h2 {
    font-size: 1.375rem;
    margin-bottom: 2rem;
    font-weight: bold;
  }

  textarea {
    min-height: 250px;
  }
}

#atencionUsuario,
#solicitudBac,
#solicitudBG,
#preinscripcion {
  .form-inline {
    .form-group {
      margin-bottom: 1rem;
    }
  }

  .custom-select {
    @include prefix(appearance, none);
  }

  .custom-radio,
  .custom-file,
  .custom-checkbox,
  .form-check {
    label {
      font-weight: 400;
      font-size: 1rem;
    }
  }

  label {
    font-weight: bold;
    font-size: 0.75rem;
  }

  #minorias,
  #dsi,
  #invColaborativo,
  #invExterno {
    display: none;
  }

  i {
    margin: 0;
  }

  select {
    position: relative;
    //overflow: hidden;
  }

  .custom-file-label {
    &::after {
      font-family: "icoagrosavia";
      content: "\e92a"; //icon-normas
    }
  }

  .button-slider {
    font-size: 1rem;
  }

  .invalid-feedback {
    display: block;
  }
}

.input-group {
  &.animate {
    flex-direction: row-reverse;

    .form-control,
    select {
      border-top-left-radius: 0;
      border-bottom-left-radius: 0;
      border-top-right-radius: $input-border-radius !important;
      border-bottom-right-radius: $input-border-radius !important;
    }

    .input-group-prepend {
      .input-group-text {
        border-top-left-radius: $input-border-radius;
        border-bottom-left-radius: $input-border-radius;
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
        margin: 0;
        @include prefix(transition, all 0.3s ease);
      }
    }

    input,
    select,
    textarea {
      @include prefix(transition, all 0.3s ease);

      &:focus {
        border-color: rgba($ot, 0.8);
        box-shadow: 0 1px 1px rgba($black, 0.075) inset, 0 0 8px rgba($ot, 0.6);
        outline: 0 none;

        +.input-group-prepend {
          .input-group-text {
            background-color: $ot;
            border-color: rgba($ot, 0.8);
            color: $white;
          }
        }
      }
    }
  }
}

/* animacion mensaje enviado */
#send-message {
  position: relative;
  background-color: #163040;
  background-image: url('data:image/svg+xml;utf8,<svg viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg"><circle opacity="0.5" fill="%23FFFFFF" cx="28.75" cy="22.338" r="0.713"/><circle opacity="0.2" fill="%23FFFFFF" cx="26.75" cy="79.443" r="0.307"/><circle opacity="0.5" fill="%23FFFFFF" cx="50" cy="41.884" r="0.308"/><circle fill="%23FFFFFF" cx="12.883" cy="42.25" r="0.367"/><circle opacity="0.4" fill="%23FFFFFF" cx="80" cy="72.818" r="0.307"/><circle opacity="0.5" fill="%23FFFFFF" cx="55.625" cy="12.375" r="0.308"/><circle opacity="0.2" fill="%23FFFFFF" cx="73.307" cy="89.375" r="0.308"/><circle opacity="0.5" fill="%23FFFFFF" cx="73" cy="53.884" r="0.308"/><circle opacity="0.5" fill="%23FFFFFF" cx="38.875" cy="89.432" r="0.385"/></svg>');
  background-size: 20rem 20rem;
  background-position: center 0;
  overflow: hidden;
  height: 400px;
  margin-right: -20px;
  margin-left: -20px;
  animation: bg 2s linear infinite;

  h2 {
    color: $white;
    position: relative;
    top: 0;
    margin: 1.5rem 0 2rem !important;
    text-align: center;
  }
}

.ship {
  position: relative;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 6.3rem;
  height: 6.7rem;
  margin: auto;
  animation: mover 2s linear infinite alternate;

  .body {
    position: relative;
    height: 110px;

    img {
      width: 100px;
    }

    .eyes {
      width: 18px;
      position: relative;
      top: -81px;
      left: 39px;
      transform: rotate(6deg);
      animation: eyes 2s ease-in-out infinite alternate;

      .eye_1,
      .eye_2 {
        position: absolute;
        display: block;
        width: 0.3rem;
        height: 0.4rem;
        border-radius: 50%;
        background-color: #543018;
        animation: eye 2s ease-in-out infinite alternate;
      }

      .eye_2 {
        right: 0;
      }
    }

    .vawes {
      position: relative;
      top: -8rem;
      left: 2rem;
      width: 3rem;
      height: 3rem;
      fill: none;
    }

    .wave_1,
    .wave_2 {
      stroke: rgba(227, 227, 227, 0.7);
      stroke-width: 1;
      stroke-linecap: round;
      stroke-linejoin: round;
      stroke-dasharray: 2;
      animation: wave 2s infinite linear;
      opacity: 0;
      transform-origin: center;
      animation-delay: 0.1s;
    }

    .wave_2 {
      animation-delay: 1s;
    }
  }
}

/* ------------------------------------------------------------------- 
 * Preguntas frecuentes - acordion
 * ------------------------------------------------------------------- */

#accordion {
  .title {
    font-weight: 700;
    padding: 0.375rem 1.5rem 0.375rem 0.75rem;
    font-size: 1rem;
    line-height: 1.5;
  }

  .card {
    counter-increment: faq-list;
    position: relative;
    width: 100%;

    button {
      border: none;
      border-bottom: $card-border-width solid $card-border-color;

      &:focus {
        outline: 0;
        outline-offset: -2px;
      }

      &::after {
        content: "\e942"; //icon-up-arrow
        font-size: 1.1rem;
        color: $modCyan;
        font-family: "icoagrosavia";
        position: absolute;
        right: 0;
        margin-right: 1.25rem;
        transform: rotate(180deg);
        @include prefix(transition, all 0.5s ease);
      }

      &.active {
        &::after {
          transform: rotate(0deg);
          @include prefix(transition, all 0.5s ease);
        }
      }
    }

    .card-header {
      &:before {
        content: counter(faq-list, decimal-leading-zero);
        background: $white;
        border-radius: 50%;
        font-weight: bold;
        padding: 0.2rem 0.37rem;
      }
    }

    .centered {
      justify-content: flex-start;
      text-align: left;
    }
  }

  @include media-breakpoint-down(md) {
    .title {
      padding: 0 0.75rem;
    }
  }
}

/* ------------------------------------------------------------------- 
 * Notificaiones - card
 * ------------------------------------------------------------------- */

#notificaiones {
  .card {
    padding: 0 0.5rem;
    box-shadow: 17px 16px 52px rgba(0, 0, 0, 0.3);
    margin-top: 100px;
    background-color: $limeGreen;
    border-radius: 5px;

    .card-header {
      background: #142736;
      padding: 1.2rem 1rem;
      box-shadow: 20px 20px 20px rgba(0, 0, 0, 0.1);
      border-radius: 5px;
      position: relative;
      top: -40px;

      h3,
      h6 {
        text-align: center;
        color: $gray-100;
        margin: 0 !important;
      }

      .noti {
        color: $yellow;
      }

      .pull-right {
        text-align: center;
      }
    }

    .card-body {
      padding-top: 0;

      h2 {
        text-align: center;
        text-transform: uppercase;
        margin-top: 0 !important;
      }
    }

    @include media-breakpoint-up(md) {
      padding: 0 3rem;

      .card-header {

        h3,
        h6 {
          text-align: left;
        }

        .pull-right {
          text-align: right;
        }
      }
    }
  }
}

/* ------------------------------------------------------------------- 
 * Nosotros - REDES int
 * ------------------------------------------------------------------- */
ul.lista-lineamientos {
  padding: 0;
  margin: 0;

  // cajas total
  li {
    list-style: none;

    &:nth-child(odd) {
      background-color: $gray-100;

      ul {
        li {
          &::after {
            background-color: $gray-100 !important;
          }
        }
      }
    }

    // caja interna
    ul {
      display: flex;
      flex-direction: column;
      margin: 1.5rem 0;
      padding: 1rem 0.75rem;

      @include media-breakpoint-up(md) {
        flex-direction: row;
        align-items: stretch;
      }

      // izq y derecha
      li {
        background-color: transparent !important;
        margin: 0 !important;
        padding: 0 !important;
        position: relative;
        flex: 1;

        // caja izquierda
        &:first-child {
          border-right: 1px solid rgba(193, 193, 193, 0.8);
          padding-right: 2em !important;
          //padding-left: 0;

          &::after {
            content: "\e920"; //icon-leaf
            color: $v-agro;
            font-size: 1rem;
            font-family: "icoagrosavia";
            position: absolute;
            right: -24px;
            top: calc(50% - 28px);
            padding: 1rem;
            background-color: $white;
          }

          ul {
            li {
              border: none;
              padding: 0 !important;

              &:first-child {
                margin-bottom: 1rem !important;
              }
            }
          }
        }

        //caja derecha
        &:last-child {
          padding-left: 2rem !important;

          ul {
            li {
              border: none;
              padding: 0 !important;

              ul {
                li {
                  display: flex;

                  &::before {
                    content: "\e930"; //icon-plant
                    font-size: 0.8rem;
                    font-family: "icoagrosavia";
                    //counter-increment: section;
                    //content: counters(section, ".") " ";
                    margin-right: 5px;
                  }
                }
              }
            }
          }
        }

        @include media-breakpoint-down(md) {
          &:first-child {
            border-right: none;
            border-bottom: 1px solid rgba(193, 193, 193, 0.8);
            padding: 0 0 2rem !important;

            &::after {
              right: calc(50% - 24px);
              top: calc(100% - 28px);
            }
          }

          &:last-child {
            padding: 2rem 0 0 !important;

            ul {
              padding-top: 0 !important;

              li {
                flex-direction: row;
              }
            }
          }
        }

        // info total
        ul {
          display: block;
          margin: 0;
          padding: 0;

          li {
            &:first-child {
              font-weight: bold;
              line-height: 1.7em;

              &::after {
                display: none;
              }
            }

            ul {
              margin-top: 1rem !important;

              li {
                font-weight: 400 !important;
                margin-bottom: 1rem !important;

                &:last-child {
                  margin-bottom: 0 !important;
                }
              }
            }
          }
        }
      }
    }
  }
}

/*division mision y vision*/
.division-txt {
  display: flex;
  flex-direction: column;
  margin: 3rem 0;

  @include media-breakpoint-up(md) {
    flex-direction: row;
    align-items: stretch;
  }
}

.division-txt-item {
  flex: 1;

  @include media-breakpoint-up(md) {
    padding-left: 1em;

    &:first-child {
      padding: 0;
    }
  }
}

.division-txt-separation {
  display: flex;
  flex-direction: row;
  align-items: center;

  i {
    margin: 0;
    color: $v-agro;
  }

  @include media-breakpoint-up(md) {
    flex-direction: column;
  }
}

.division-txt-label {
  padding: 1em;
}

.division-txt-rule {
  flex: 1;
  border-style: solid;
  border-color: rgba($gray-bg, 0.8);
  border-width: 1px 0 0 0;

  @include media-breakpoint-up(md) {
    border-width: 0 1px 0 0;
  }
}

/*propiedades*/
.propiedades-redes {
  margin: 2rem 0 3rem;

  .row {
    position: relative;
  }

  .leaf {
    padding-top: 15px;
    padding-bottom: 15px;
    display: flex;
    @include prefix(transition, all 0.3s ease-in-out);

    &:nth-child(odd) {
      justify-content: flex-end;

      &::after {
        content: "";
        background: repeating-linear-gradient(-45deg,
            $tierra 0,
            $tierra 2px,
            transparent 0,
            transparent 5px);
        width: 10px;
        border-radius: 0.2rem;
        height: auto;
        display: block;
        min-height: 260px;
        position: absolute;
        top: 0;
        right: -5px;
      }
    }

    &:nth-child(2n) {
      justify-content: flex-start;

      h3 {
        border-radius: 50% 0;
        @include gradient-box-border($surco, $ot, 90deg);
      }
    }

    a {
      text-decoration: none;
    }

    @include media-breakpoint-down(xs) {
      &:nth-child(odd) {
        justify-content: center;

        &::after {
          content: none;
        }
      }

      &:nth-child(2n) {
        justify-content: center;
      }
    }
  }

  h3 {
    margin: 0 !important;
    border: solid 0.2rem transparent;
    padding: 3rem;
    border-radius: 0 50%;
    box-shadow: inherit;
    @include gradient-box-border($ot, $surco, 90deg);
    color: #fff;
    font-weight: 400 !important;
    line-height: 1.5rem !important;
    text-align: center;
    min-width: 240px;
    max-width: 280px;

    &[data-many] {
      &::before {
        content: attr(data-many);
        color: $white;
        display: block;
        font-size: 5rem !important;
        line-height: 5rem;
        font-weight: 900 !important;
        text-shadow: -2px -1px $surco;
      }
    }
  }
}

/* objetivos redes */
.objetivos {
  position: relative;
  margin-top: 2rem;

  ul {
    li {
      color: $white;
      margin-bottom: 2px;
      position: relative;
      top: -2rem;
      @include prefix(transition, all 0.52s ease-in-out);

      @for $i from 1 through 100 {
        $j: 100 - $i + 1; // reverse nth-child order math

        &:nth-child(#{$i}) {
          z-index: $j;
        }
      }

      &:first-child {
        background-color: $limeGreen !important;
        color: $body-color;
        top: 0;
        padding: 0;
        z-index: 1;

        .item-obj {
          text-align: center;
          padding: 4rem 0 6rem;

          h2 {
            margin: 0 !important;
          }

          &::before,
          &::after {
            content: none;
          }
        }
      }

      &:nth-child(2) {
        .item-obj {
          &::before {
            content: none;
          }
        }
      }

      &:nth-child(even) {
        background: $a-savia;

        .item-obj {
          &:after {
            border-top: 50px solid $a-savia;
          }
        }
      }

      &:nth-child(odd) {
        background: $a-savia-700;

        .item-obj {
          &:after {
            border-top: 50px solid $a-savia-700;
          }
        }
      }

      &:last-child {
        .item-obj {
          &::after {
            content: none;
          }
        }
      }
    }

    @include media-breakpoint-down(md) {
      &.row {
        margin-right: -20px;
        margin-left: -20px;
      }

      li {
        top: 0;

        &:first-child {
          .item-obj {
            padding-bottom: 4rem;
          }
        }
      }
    }
  }
}

.item-obj {
  position: relative;
  padding: 4.375rem 0.313rem 1.25rem 0.313rem;

  &::after {
    content: "";
    display: block;
    border-left: 60px solid transparent;
    border-right: 60px solid transparent;
    border-top: 50px solid transparent;
    position: absolute;
    left: calc(50% - 60px);
    z-index: 13;
    bottom: -50px;
  }

  &::before {
    content: "";
    display: block;
    border-left: 60px solid transparent;
    border-right: 60px solid transparent;
    border-top: 50px solid $white;
    position: absolute;
    left: calc(50% - 60px);
    z-index: 10;
    top: 0;
  }
}

/* bar charts */
.chart {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 2rem;

  #numbers {
    width: 40px;
    margin: 0;
    padding: 0 1px;
    display: flex;
    flex-direction: column;

    li {
      text-align: right;
      padding-right: 1em;
      list-style: none;
      height: 29px;
      border-bottom: 1px solid #444;
      position: relative;
      bottom: 30px;

      &:last-child {
        height: 30px;
      }

      span {
        color: #eee;
        position: absolute;
        bottom: 0;
        right: 10px;
      }
    }
  }

  .bars {
    height: 290px;
    padding: 0;
    margin: 0;
    border-bottom: 1px solid $gray-500;

    li {
      display: table-cell;
      width: 80px;
      height: 290px;
      margin: 0;
      text-align: center;
      position: relative;

      [class^="bar-"],
      [class*=" bar-"],
      .bar {
        display: block;
        margin-left: 15%;
        width: 70%;
        background: #49e;
        position: absolute;
        bottom: 0;
        opacity: 1;
        @include prefix(transition, all 0.3s ease-in-out);

        &:before {
          color: $body-color;
          content: attr(data-percentage);
          position: relative;
          bottom: 25px;
          opacity: 0.5;
          @include prefix(transition, all 0.3s ease-in-out);
        }

        p {
          line-height: 0;
          margin: 0;
          opacity: 0;
        }
      }

      &:nth-of-type(1) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $yellow;
        }
      }

      &:nth-of-type(2) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $orange;
        }
      }

      &:nth-of-type(3) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $red;
        }
      }

      &:nth-of-type(4) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $pink;
        }
      }

      &:nth-of-type(5) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $purple;
        }
      }

      &:nth-of-type(6) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $indigo;
        }
      }

      &:nth-of-type(7) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $blue;
        }
      }

      &:nth-of-type(8) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $cyan;
        }
      }

      &:nth-of-type(9) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $teal;
        }
      }

      &:nth-of-type(10) {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          background: $green;
        }
      }

      &:hover {

        [class^="bar-"],
        [class*=" bar-"],
        .bar {
          opacity: 0.8;

          &:before {
            opacity: 1;
          }
        }
      }
    }
  }
}

.bars-description {
  font-size: 0.75rem;
  margin-top: 1rem;
  margin-bottom: 0;

  li {
    display: flex;
    align-items: center;

    &:before {
      content: "";
      height: 10px;
      width: 10px;
      border-radius: 3px;
    }

    &:nth-of-type(1) {
      &:before {
        background: $yellow;
      }
    }

    &:nth-of-type(2) {
      &:before {
        background: $orange;
      }
    }

    &:nth-of-type(3) {
      &:before {
        background: $red;
      }
    }

    &:nth-of-type(4) {
      &:before {
        background: $pink;
      }
    }

    &:nth-of-type(5) {
      &:before {
        background: $purple;
      }
    }

    &:nth-of-type(6) {
      &:before {
        background: $indigo;
      }
    }

    &:nth-of-type(7) {
      &:before {
        background: $blue;
      }
    }

    &:nth-of-type(8) {
      &:before {
        background: $cyan;
      }
    }

    &:nth-of-type(9) {
      &:before {
        background: $teal;
      }
    }

    &:nth-of-type(10) {
      &:before {
        background: $green;
      }
    }

    a {
      color: $body-color;
      margin-left: 0.313rem;
    }
  }
}

/* Modal full redes */
[id^="modal-full-"],
[id*="modal-full-"] {
  padding: 0 !important;

  .modal-dialog {
    max-width: 80%;

    /*margin: 0;*/
    &.modal-full {
      background: $white;
      width: 100%;
      height: 100%;
      margin: 0;
      padding: 0;
    }

    .modal-content {
      height: auto;
      min-height: 100%;
      border-radius: 0;
      border: none;

      .container {
        background: none;
        margin-top: 0;
        margin-bottom: 0;

        .md-title {
          margin-top: 0 !important;
          margin-bottom: 0 !important;
        }
      }

      .modal-header {
        display: block;
        padding: 0;
        border-radius: 0;

        header {
          width: 100%;
        }
      }

      .modal-body {
        h2 {
          font-size: 1.375rem;
          margin-top: 45px;
          margin-bottom: 5px;
          line-height: 35px;
          font-weight: bold;
        }

        @include media-breakpoint-down(md) {
          .container {
            padding-right: 0;
            padding-left: 0;
          }
        }
      }

      .modal-footer {
        border-top: none;
      }

      .linea {
        .division-txt {
          margin: 1.5rem 0;
          padding: 1rem 0.75rem;

          &:nth-child(odd) {
            background-color: rgba(0, 0, 0, 0.05);
          }

          .division-txt-item {
            b {
              margin-bottom: 1rem;
              display: block;
            }
          }
        }
      }
    }
  }

  @include media-breakpoint-down(sm) {
    .modal-dialog {
      max-width: 100%;
    }
  }
}

.fade-scale {
  transform: scale(0);
  opacity: 0;
  @include prefix(transition, all 0.4s linear);

  &.show {
    opacity: 1;
    transform: scale(1);
  }
}

