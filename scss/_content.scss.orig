.parallax-work {
  width: 100%;
  height: 100%;
  min-height: 586px;
  background-color: transparent;
  position: relative;
  display: flex;
  overflow: hidden;
  /*display: table;*/

  .shadow-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0.4;
    background: -moz-linear-gradient(top,
        transparent 0%,
        rgba(0, 0, 0, 0.8) 100%);
    background: -webkit-linear-gradient(top,
        transparent 0%,
        rgba(0, 0, 0, 0.8) 100%);
    background: linear-gradient(to bottom,
        transparent 0%,
        rgba(0, 0, 0, 0.8) 100%);

    /*-webkit-filter: blur(2px); 
    filter: blur(2px);*/
  }

  .overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0.6;
    background-color: #000000;
  }
}

.parallax-content {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 8rem 0;
  position: relative;
  overflow: hidden;

  h1,
  h3 {
    color: $white;
    letter-spacing: 0.3rem;
    text-transform: uppercase;
    text-align: center;
    margin-bottom: 30px;
  }

  h1 {
    position: relative;
    margin-bottom: 80px;

    &::after {
      content: "";
      position: absolute;
      left: calc(50% - 1.2em);
      bottom: -30px;
      height: 2px;
      width: 2.4em;
      background: $white;
      /*transform: translateY(-50%);*/
    }
  }

  p {
    color: $white;
  }

  @include media-breakpoint-down(md) {
    padding: 3rem 0;

    h1 {
      font-size: 2rem;
    }
  }
}

.work-content-main {
  padding-top: 2rem;
  position: relative;

  img {
    border-color: #f00;
    border-width: 10px;
  }

  .groove {
    border-style: groove;
  }

  .alert {
    color: $white;
  }

  a {
    &:hover {
      text-decoration: none;

      .box-container {
        .box-3d {
          transform: perspective(0) rotateX(0) rotateZ(0);

          .box {
            &.box2 {
              top: 10px;
              left: 0;
            }

            &.box3 {
              top: 20px;
              left: 0;
            }
          }
        }
      }
    }

    &#c-trabaje {
      [data-count] {
        &::after {
          @include alert-count(data-count, $purple-light, $white, 25%, 25%);
        }
      }
    }

    &#c-contratacion {
      [data-count] {
        &::after {
          @include alert-count(data-count, $blue-light, $white, 25%, 25%);
        }
      }
    }
  }
}

.box-container {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  height: 200px;

  .box-3d {
    transform-style: preserve-3d;
    transform: perspective(1000px) rotateX(65deg) rotateZ(45deg);
    height: auto;
    width: 100px;
    position: absolute;
    margin: 0 auto;
    left: 0px;
    right: 0px;
    @include prefix(transition, all 0.3s ease);

    .box {
      @include gradient-directional($purple-dark, $purple-light, 125deg);
      width: 100px;
      height: 100px;
      text-align: center;
      padding: 10px;
      position: relative;
      @include prefix(box-shadow, 0px 3px 30px 17px rgba(0, 0, 0, 0.3));
      @include prefix(transition, all 0.3s ease);
      z-index: 2;

      i {
        width: 100%;
        height: 100%;
        padding: 5px;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 2.5rem;
        color: $white;
        /*path {
          stroke-dasharray: 10;
          stroke-dashoffset: 1000;
          animation: dash 5s linear infinite;
        }*/
      }

      &.box2 {
        position: absolute !important;
        top: 25px;
        left: 25px;
        z-index: 1 !important;
        @include prefix(transition, all 0.3s ease);
      }

      &.box3 {
        position: absolute !important;
        top: 50px;
        left: 50px;
        z-index: 0 !important;
        @include prefix(transition, all 0.3s ease);
      }

      &[class^="deg-"],
      &[class*=" deg-"] {
        @include gradient-directional($blue-dark, $blue-light, 125deg);
      }
    }
  }
}

