  transform: scale(1.1, 1.1);
}*/

.card__link {
  position: absolute;
  top: 10px;
  right: 10px;
  background: transparent;
  border: 0;
  border-radius: 50%;
  height: 40px;
  width: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  @include prefix(transition, all $speed ease);

  .icon {
    font-size: 18px;
    margin: 0;
  }

  &:focus {
    outline: 0;
    background-color: rgba(255, 255, 255, 0);
  }

  i {
    font-size: 18px;
    color: rgba(255, 255, 255, 0.6);
    cursor: pointer;
    @include prefix(transition, all $speed ease);
  }

  &:hover {
    i {
      color: rgba(255, 255, 255, 0.8);
    }
  }

  &:hover,
  &:active,
  &.active {
    opacity: 1 !important;
  }

  &:active,
  &.active {
    i {
      color: $white;
    }
  }
}

.card__image {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  overflow: hidden;
  display: block;
  width: 100%;
  height: 100%;
  pointer-events: none;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 1), rgba(0, 0, 0, 0.5));

  img {
    @include prefix(transform, scale(1.3, 1.3));
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    opacity: 1;
    @include prefix(transition, all $speed ease);
    animation-name: blur;
    animation-duration: 30s;
    animation-direction: alternate;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
  }
}

.card__header {
  display: grid;
  width: 100%;
  height: 90px;

  figure {
    margin-bottom: 0 !important;
  }

  @include media-breakpoint-up(sm) {
    height: 150px;
  }
}

.card__profile {
  transform: translateY(0);
  align-self: center;
  justify-self: center;
  display: block;
  overflow: hidden;
  width: 6vmax;
  height: 6vmax;
  max-width: 100px;
  max-height: 100px;
  border-radius: 50%;
  @include prefix(transition, all $speed ease);

  img {
    @include prefix(transform, scale(1.5, 1.5) translateZ(0));
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: 50% 50%;
    /*@include prefix(filter, grayscale(50%) contrast(75%) brightness(1.3));*/
    @include prefix(transition, all $speed ease);
    mix-blend-mode: normal;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
  }
}

.center-name {
  padding: 2rem 0;

  h2 {
    margin: 0 !important;
  }

  @include media-breakpoint-down(md) {
    text-align: center;
    padding-bottom: 5rem;
  }
}

.card-director {
  background-color: #fbfbfb;
  border-radius: 1rem;
  margin-bottom: 3rem;
  position: relative;

  .row {
    .contact-director div {
      &:nth-child(2) {
        justify-content: flex-start;
      }
    }

    .info-director {
      margin-top: 1rem;
      margin-bottom: 3rem;
    }
  }

  h3 {
    margin: 0 0 1rem !important;
    line-height: 23px !important;
  }

  ul.lista {
    li {
      margin: 0 !important;

      a {
        color: $body-color;
      }
    }
  }

  .btn-float {
    position: absolute;
    bottom: 0;
    right: 0;
    margin: 1rem;
  }

  .btn {
    i {
      font-size: 1rem;
    }
  }

  @include media-breakpoint-down(sm) {
    margin-top: 5rem;

    .lista {
      margin-top: 5rem;
    }
  }

  @include media-breakpoint-down(md) {
    .row {
      .contact-director div {
        &:nth-child(2) {
          justify-content: center;
        }
      }
    }
  }
}

.card-profile {
  position: absolute;
  top: -40px;
  overflow: hidden;
  max-width: 80px;
  max-height: 80px;
  border-radius: 50%;
  z-index: 1;
  @include prefix(transition, all $speed ease);

  &.director {
    box-shadow: 0 0 0px 5px $v-agro-800;
    top: -60px;
    max-width: 120px;
    max-height: 120px;

    img {
      width: 120px !important;
      height: 120px !important;
      object-fit: cover;
      object-position: top center;
    }

    @include media-breakpoint-up(md) {
      position: relative;
      top: 0 !important;
      margin: 2rem 0;
      border-radius: 2rem;
    }
  }

  img {
    width: 100%;
  }
}

.card__body {
  display: grid;
  padding: 20px 10px;
  background: rgba($black, 0.3);
  border-radius: 10px;
  @include prefix(transition, all $speed ease);

  @include media-breakpoint-down(sm) {
    border-radius: 10px 10px 0 0;
  }
}

.card__name {
  align-self: center;
  justify-self: center;
  margin-bottom: 0;
  color: white;
  font-size: $fontMedium;
  font-weight: $fontLight;
  letter-spacing: 0.1rem;
  /*text-shadow: 0 1px 2px rgba(0, 0, 0, 0.35);*/
  background: -webkit-linear-gradient($white, $gray-100);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  @include prefix(transition, all $speed ease);

  @include media-breakpoint-up(lg) {
    padding-right: 15px;
    padding-left: 15px;
  }
}

.card__job {
  align-self: center;
  justify-self: center;
  display: inline-block;
  font-size: $fontSmall;
  font-weight: $fontLight;
  text-align: center;
  letter-spacing: 0.35rem;
  color: rgba(100, 130, 200, 0.45);
  @include prefix(transition, all $speed ease);
}

.card__bio {
  position: absolute;
  left: 0;
  bottom: 0;
  @include prefix(transform, translateY(30%));
  margin: 0;
  border-bottom: 1px solid rgba($white, 0.2);
  padding-bottom: 5px;
  font-size: $fontMedium;
  /*text-shadow: 0 0.5px 1px rgba(0, 0, 0, 0.35);*/
  color: $white;
  opacity: 0;
  display: flex;
  justify-content: center;
  line-height: 1.2em !important;
  text-align: center;
  width: 100%;
  @include prefix(transition, all $speed ease);

  @include media-breakpoint-up(sm) {
    bottom: 20px;
    border-bottom: 1px solid transparent;
    padding-bottom: 0;
  }

  @include media-breakpoint-up(xl) {
    bottom: 15px;
  }
}

.card__date {
  grid-area: date;
  display: inline-block;
  align-self: left;
  justify-self: left;
  font-size: $fontSmall;
  font-weight: $fontLight;
  text-transform: uppercase;
  text-shadow: 0 0.5px 1px rgba(0, 0, 0, 0.45);
  color: rgba(250, 250, 250, 0.4);
}

/* MODIFIERS */
.card_ot[data-effect="blur"]:hover .card__image img {
  @include prefix(filter, blur(3px));
}

.card_ot[data-effect="blur"] .card__image img {
  animation-name: blur;
}

.card_ot[data-effect="color"]:hover {
  -webkit-transform: scale(1, 1);
  transform: scale(1, 1);
  box-shadow: 0 40px 260px rgba(255, 0, 0, 0.1),
    0 40px 130px rgba(250, 100, 100, 0.2),
    -80px -40px 230px rgba(0, 200, 250, 0.15),
    80px 40px 230px rgba(120, 120, 255, 0.15);
}

.card_ot[data-effect="color"] .card__image {
  background: linear-gradient(to bottom,
      rgba(20, 20, 100, 1),
      rgba(255, 100, 100, 0.5));
}

.card_ot[data-effect="color"]:hover .card__image img {
  opacity: 0.8;
}

.card_ot[data-effect="color"]:hover .card__profile img {
  @include prefix(filter, grayscale(50%) contrast(80%) brightness(1.6));
  mix-blend-mode: normal;
}

@keyframes blur {
  from {
    object-position: 0 50%;
  }

  to {
    object-position: 100% 50%;
  }
}

@keyframes blur {
  from {
    @include prefix(transform, scale(1, 1));
  }

  to {
    @include prefix(transform, scale(1.2, 1.2));
  }
}

