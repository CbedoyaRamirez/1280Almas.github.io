/* ------------------------------- 
 * Noticias / OT
 * ------------------------------- */
.bg-header {
  background: $ot;
  background-size: 250px;
  background-position: center center;
}

.card {
  border: none;
  margin-bottom: 30px;
  transition: all 1s ease;

  .card-img-top {
    position: relative;
    @include prefix(transition, all 0.3s ease-in-out);

    img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      object-position: center center;
    }

    .bg-cover {
      @include border-top-radius($card-inner-border-radius);
      @include prefix(transition, all 0.3s ease-in-out);
    }

    .texture-overlay {
      background-image: none;

      a {
        display: block;
      }
    }

    .hover-item {
      visibility: hidden;
      opacity: 0;
      position: absolute;
      @include prefix(transition, opacity 0.3s ease-in-out);

      .bt-link {
        &:hover {
          opacity: 0.7;
        }
      }

      &.bottom {
        bottom: 20px;
      }

      &.top {
        top: 20px;
      }

      &.left {
        left: 7px;
      }

      &.right {
        right: 20px;
      }

      &.center {
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
      }

      .bt-link {
        width: 35px;
        height: 35px;
        border: 1px solid rgba($white, 0.5);
        border-radius: 50%;
      }

      .bt-link {
        border-radius: 50%;
        height: 40px;
        width: 40px;
        color: $white;
        display: flex;
        align-items: center;
        justify-content: center;
        @include prefix(transition, opacity 0.3s ease-in-out);

        .icon {
          font-size: 18px;
          margin: 0;
        }
      }

      .icon {
        font-size: 40px;
        color: $white;
        margin: 0;
      }

      @include media-breakpoint-down(sm) {
        opacity: 1;
        filter: alpha(opacity=100);
        /* For IE8 and earlier */
      }
    }

    &:hover {
      .bg-cover {
        transform: scale(1.05);
        @include prefix(transition, all 0.3s ease-in-out);
      }

      a {
        text-decoration: none;
      }

      /*ciencia*/
      .ci {
        .texture-overlay {
          background-color: rgba($ci, 0.7);
        }
      }

      /*POSconflicto*/
      .pos {
        .texture-overlay {
          background-color: rgba($pos, 0.7);
        }
      }

      /*Oferta Tecnologica*/
      .ot {
        .texture-overlay {
          background-color: rgba($ot, 0.7);
        }
      }

      /*EVento*/
      .ev {
        .texture-overlay {
          background-color: rgba($ev, 0.7);
        }
      }

      /*Redes de Innovacion*/
      .ri {
        .texture-overlay {
          background-color: rgba($indigo, 0.7);
        }
      }

      /*Informacion Oficial*/
      .io {
        .texture-overlay {
          background-color: rgba($io, 0.7);
        }
      }

      /*SUbasta*/
      .su {
        .texture-overlay {
          background-color: rgba($orange, 0.7);
        }
      }

      /*INservibles*/
      .in {
        .texture-overlay {
          background-color: rgba($in, 0.7);
        }
      }

      /*No Aplica*/
      .na {
        .texture-overlay {
          background-color: rgba($a-savia, 0.7);
        }
      }

      .hover-item {
        visibility: visible;
        opacity: 1;
      }
    }
  }

  &:hover {
    box-shadow: 2px 2px 20px rgba(0, 0, 0, 0.3);
  }
}

