/* Libros */
.book {
  position: relative;

  .pages {
    display: block;
    width: 100%;
    position: relative;
    background: $white;
    z-index: 1;
    box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 9px 20px 0 rgba(0, 0, 0, 0.25);
    overflow: hidden;
    @include prefix(transition, box-shadow 0.3s linear);

    img {
      width: inherit;
      height: inherit;
      @include prefix(transform-origin, 0 50%);
      @include prefix(transform, rotateY(0));
      @include prefix(transition, all 0.45s ease);
    }

    &::before,
    &::after {
      content: "";
      display: block;
      width: 100%;
      height: 100%;
      position: absolute;
      z-index: -1;
      top: 0;
      left: 0;
      background: white;
      border: 1px solid #d9d9d9;
    }

    &::before {
      left: -3px;
    }

    &::after {
      left: -7px;
    }
  }

  .hover-book {
    position: absolute;
    bottom: 0;
    z-index: 1;
    background: rgba($gray-900, 0.8);
    margin: 10px;
    border-radius: 10px;
    visibility: hidden;
    opacity: 0;
    @include prefix(transition, all 0.45s ease);

    a,
    i {
      margin: 0;
    }
  }

  .pages,
  .pages img,
  .pages::after,
  .pages::before {
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
  }

  &:hover {
    .pages {
      box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.25),
        0 9px 20px 0 rgba(0, 0, 0, 0.45);

      img {
        @include prefix(transform, rotateY(-25deg));
        box-shadow: 1px 1px 5px 5px rgba(0, 0, 0, 0.2);
      }
    }

    .hover-book {
      visibility: visible;
      opacity: 1;
    }
  }
}

