/* ------------------------------ section pectia, siembra y linkata ---------------------------------- */

.content-sites {
  figure {
    position: relative;
    float: left;
    overflow: hidden;
    background: #3085a3;
    text-align: center;
    cursor: pointer;
    margin-bottom: 0;

    img {
      position: relative;
      display: block;
      min-height: 100%;
      max-width: 100%;
      opacity: 0.8;
    }

    figcaption,
    igcaption>a {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }

    figcaption {
      padding: 2em;
      color: #fff;
      -webkit-backface-visibility: hidden;
      backface-visibility: hidden;
    }
  }

  .effect-ruby {
    background-color: #17819c;

    img {
      opacity: 0.7;
      transition: opacity 0.35s, transform 0.35s;
      transform: scale(1);
    }

    figcaption {
      img {
        opacity: 1;
        max-width: 170px;
        min-height: auto;
        position: absolute;
        /*display: flex;
        align-items: center;
        justify-content: center;*/
        left: calc(50% - 85px);
        bottom: 20%;
        @include prefix(transition, all 0.3s ease-in-out);
      }

      .btn {
        position: absolute;
        bottom: 0;
        opacity: 0;
        transform: scale(1.2);
        @include prefix(transition, all 0.3s ease-in-out);
      }
    }

    &:hover {
      img {
        opacity: 0.5;
        transform: scale(1.15);
      }

      figcaption {
        img {
          opacity: 1;
          transform: scale(1);
          max-width: 170px;
          min-height: auto;
          bottom: 30%;
        }

        .btn {
          bottom: 10%;
          opacity: 1;
          transform: scale(1);
        }
      }
    }
  }
}

