/* ------------------------------------------------------------------- 
 * Explicacion sub menu
 * ------------------------------------------------------------------- */

.explain-me-how {
  position: absolute;
  bottom: -10px;
  right: 0;
  height: auto;
  width: 200px;
  @include prefix(animation, show-hand 6s both);

  .text {
    text-align: center;
    color: $white;
    padding: 20px;
    text-transform: uppercase;
    font-size: 18px;
    font-weight: 400;
    text-shadow: 0px 0px 10px rgba(0, 0, 0, 0.75);
  }

  .track {
    position: relative;
    height: auto;
    width: 100%;
  }

  .hand {
    z-index: auto;
    left: 0;
    top: 0;
    width: 52px;
    height: 52px;
    position: relative;
    padding: 10px;
    @include prefix(animation, move-the-hand 3s both infinite);

    i {
      display: block;
      position: relative;
      z-index: 2;
      font-size: 2rem;
    }
  }

  .hand__finger-touch {
    position: absolute;
    z-index: 1;
    width: 20px;
    height: 20px;
    border-radius: 50%;
    background-color: $ot;
    left: 10px;
    top: 2px;
    opacity: 0;
    @include prefix(animation, show-the-touch 3s both infinite);
  }
}

/* ------------------------------------------------------------------- 
 * Sociedad
 * ------------------------------------------------------------------- */

/*Balance social*/

.box-balance {
  .card {
    .card-body {
      img {
        object-fit: cover;
        object-position: top;
        height: 300px;
        width: 100%;
      }

      .video-balance {
        overflow: hidden;

        a {
          display: block;
          @include prefix(transition, all 0.3s ease-in-out);
          cursor: pointer;

          .hover-item {
            visibility: visible;
            opacity: 1;

            i {
              color: $yt;
            }
          }

          &:hover {
            .bg-cover {
              transform: scale(1.05);
              @include prefix(transition, all 0.3s ease-in-out);
            }

            .texture-overlay {
              background-color: rgba($a-savia, 0.7);
              height: 100%;
              width: 100%;
            }

            .hover-item {
              visibility: visible;
              opacity: 1;

              i {
                color: $white;
              }
            }
          }
        }
      }

      i {
        margin: 0;
      }

      ul {
        li {
          margin: 0 0.5rem !important;

          a {
            margin: 0.5rem;
            color: $body-color;
            @include prefix(transition, all 0.3s ease-in-out);

            &:hover,
            &:active {
              .iconlink {
                i {
                  background-color: $ot;
                }
              }
            }
          }

          &:nth-child(even) {
            .mIcon {
              background-color: $agua;
            }
          }
        }
      }
    }
  }

  .cardBg {
    background-color: $gray-event;
  }

  .cardDarkBg {
    background-color: darken($gray-event, 10%);
  }

  .contactButton {
    position: absolute;
    transform: translate(-50%, -50%);
    left: 50%;
    top: 100%;
    z-index: 2;
    //width: 145px;
    height: 38px;
    @include prefix(transition, all 0.35s ease);
    overflow: hidden;

    .infoIcon {
      display: none;
    }

    &[aria-expanded="true"] {
      border-radius: 50%;
      left: 90%;
      transform: translate(-90%, -50%);
      width: 52px;
      height: 52px;

      .infoText {
        display: none;
      }

      .infoIcon {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        order: 1;
        height: 30px;
        width: 30px;
      }
    }
  }

  .mIcon {
    display: inline-flex;
    height: 40px;
    width: 40px;
    align-items: center;
    justify-content: center;
    border-radius: 50px;
    color: #ffffff;
    background-color: $cielo;
  }

  @include media-breakpoint-down(lg) {
    .card {
      .card-body {
        img {
          height: auto;
        }
      }
    }
  }
}

[id^="videoModal-"],
[id*="videoModal-"] {
  .modal-dialog {
    max-width: 800px;
    margin: 30px auto;

    .embed-responsive {
      margin-bottom: 0;
    }
  }

  .modal-body {
    position: relative;
    padding: 0px;
  }

  .close {
    position: absolute;
    right: 20px;
    top: 0;
    z-index: 999;
    font-size: 2rem;
    font-weight: normal;
    color: #fff;
    opacity: 1;
  }
}

/* ------------------------------------------------------------------- 
 * Transparencia
 * ------------------------------------------------------------------- */
#cards-transparencia {

  [class^="col-"],
  [class*=" col-"] {
    padding: 15px;
  }

  a {
    &:hover {
      text-decoration: none;
    }
  }

  .card {
    position: relative;
    height: auto;
    overflow: hidden;
    border-radius: 15px;
    margin: 0 auto;
    padding: 40px 20px;
    box-shadow: 0 10px 15px rgba(0, 0, 0, 0.3);
    transition: 0.5s;
  }

  .card:hover {
    transform: scale(1.1);
  }

  .card:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 40%;
    background: rgba(255, 255, 255, 0.1);
    z-index: 1;
    transform: skewY(-5deg) scale(1.5);
  }

  .title {
    margin-bottom: 1rem;

    .fa {
      color: $white;
      //font-size: 60px;
      width: 100px;
      height: 100px;
      border-radius: 50%;
      text-align: center;
      box-shadow: 0 10px 10px rgba(0, 0, 0, 0.1);

      img {
        width: 60%;
        height: 60%;
        object-fit: cover;
        object-position: center center;
      }
    }

    [class^="icon-"],
    [class*="icon-"] {
      font-size: 60px;
      line-height: 100px;
      margin: 0;
    }

    h2 {
      position: relative;
      margin: 0 0 0.8rem;
      padding: 0;
      color: $white;
      font-size: 18px;
      line-height: 1.2;
    }
  }

  .option {
    position: relative;

    p {
      color: $white;
      margin: 0;
    }

    ul {
      margin: 0;
      padding: 0;

      li {
        margin: 0 0 5px;
        padding: 0;
        list-style: none;
        color: $white;
        font-size: 16px;

        &::before {
          font-family: "icoagrosavia";
          content: "\e90e"; //icon-check-ok
          margin-right: 5px;
        }
      }
    }
  }

  .card .btn-card {
    display: block;
    position: relative;
    background-color: $white;
    color: $body-color;
    width: 150px;
    height: 40px;
    text-align: center;
    margin: 20px auto 0;
    line-height: 40px;
    border-radius: 40px 0.3125rem;
    //border-radius: 40px;
    font-size: 16px;
    cursor: pointer;
    text-decoration: none;
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
  }
}

#directorio {
  .box {
